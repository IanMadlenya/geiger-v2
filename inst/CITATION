citHeader("To cite auteur in a publication use:")

citEntry(entry="Article",
	title = "A novel comparative method for identifying shifts in the rate of character evolution on trees",
        author = personList(as.person("JM Eastman"),as.person("ME Alfaro"), as.person("P Joyce"),as.person("AL Hipp"),as.person("LJ Harmon")),
	journal = "Evolution",
  	year = "2011",
	volume = "65",
	pages = "3578-3589",

        textVersion = "Eastman Jonathan M, Michael E Alfaro, Paul Joyce, Andrew L Hipp, and Luke J Harmon. 2011. A novel comparative method for identifying shifts in the rate of character evolution on trees. Evolution 65:3578-3589.")

citHeader("To cite geiger in a publication use:")

citEntry(entry="Article",
	title = "GEIGER: investigating evolutionary radiations",
        author = personList(as.person("LJ Harmon"),as.person("JT Weir"), as.person("CD Brock"),as.person("RE Glor"),as.person("W Challenger")),
	journal = "Bioinformatics",
  	year = "2008",
	volume = "24",
	pages = "129-131",

        textVersion = "Harmon Luke J, Jason T Weir, Chad D Brock, Richard E Glor, and Wendell Challenger. 2008. GEIGER: investigating evolutionary radiations. Bioinformatics 24:129-131.")
